{
  "name": "reared-amipopular",
  "version": "1.0.0",
  "description": "A reactjs with redux project.",
  "main": "index.js",
  "scripts": {
    "start": "npm run build",
    "build": "webpack -d && xcopy \"src/index.html\" \"dist/\" /F /Y && webpack-dev-server --content-base src/ --inline --watch --history-api-fallback",
